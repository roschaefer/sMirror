{
  "name": "displays",
  "version": "0.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "js": "webpack --config webpack.config.js",
    "css": "postcss assets/src/css/main.css -m -o assets/dist/css/main.css",
    "build": "npm run js && npm run css",
    "start": "npm-watch"
  },
  "author": "Till Prochaska <mail@tillprochaska.de>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "npm-watch": "^0.3.0",
    "postcss": "^7.0.6",
    "postcss-cli": "^6.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.0.0",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "moment": "^2.20.1",
    "rss-parser": "^2.11.0"
  },
  "watch": {
    "js": "assets/src/js/**/*.js",
    "css": {
      "patterns": [
        "assets/src/css/**/*.css"
